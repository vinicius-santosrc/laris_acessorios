{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.ts"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/index.ts"
        },
        {
            "source": "/shipping/calculate",
            "destination": "/index.ts"
        },
        {
            "source": "/(config|create-payment-intent)",
            "destination": "/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.ts"
        }
    ]
}